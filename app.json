{
  "expo": {
    "name": "LXXN PAD",
    "slug": "lxxn-pad",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/lxxn-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/lxxn-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.teletubi1709.lxxnpad"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lxxn-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.teletubi1709.lxxnpad",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Permitir a LXXN PAD grabar audio para sampling."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "LXXN PAD necesita acceso a la cámara para el sensor óptico del Teremín."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ce2df796-eddb-4f98-a1ef-b6c3878b4539"
      }
    },
    "scheme": "lxxnpad"
  }
}